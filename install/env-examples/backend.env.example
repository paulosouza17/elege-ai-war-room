# ── Server ──
NODE_ENV=production
PORT=3000

# ── Supabase ──
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# ── Redis (BullMQ) ──
REDIS_URL=redis://localhost:6379

# ── Ingestion API Key (generate with: openssl rand -hex 32) ──
INGESTION_API_KEY=change-me-to-a-random-string

# ── Elege.ai API ──
ELEGEAI_API_TOKEN=your-token-here
